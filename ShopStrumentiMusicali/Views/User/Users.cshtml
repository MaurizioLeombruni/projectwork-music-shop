@using Microsoft.AspNetCore.Identity;
@model ICollection<IdentityUser>

@{
    ViewData["Title"] = "User";
}
<div class="text-center">
    <h1 class="display-4">Utenti:</h1>
    @if (@Model != null && @Model.Any())
    {
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Email</th>
          <th scope="col">Edit</th>
        </tr>
      </thead>
      <tbody>
    
        @foreach(var user in Model){
        <tr>
          <th scope="row">@user.Id</th>
          <td>@user.Email</td>
          <td>
              <a asp-controller="User" asp-action="Edit" asp-route-id="">Edit</a>
          </td>
        
        </tr>
        }
      </tbody>
    </table>
    }
</div>
